<bean name="beans">
  <factory>
    <null/>
  </factory>

  <bean name="b1">
    <factory>
      <string>str</string>
    </factory>
  </bean>

  <bean name="b2">
    <factory>
      <call method="valueOf">
        <target>
          <class class="java.math.BigInteger"/>
        </target>
        <args>
          <const type="LONG" value="1"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b3">
    <factory>
      <call method="new">
        <target>
          <class class="java.util.ArrayList"/>
        </target>
        <args>
          <const type="INT" value="10"/>
        </args>
        <initializers>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <const type="INT" value="2"/>
            </args>
          </call>
        </initializers>
      </call>
    </factory>
  </bean>

  <bean name="b4">
    <factory>
      <call method="size">
        <target>
          <call method="singletonList">
            <target>
              <class class="java.util.Collections"/>
            </target>
            <args>
              <const type="LONG" value="12"/>
            </args>
          </call>
        </target>
      </call>
    </factory>
  </bean>

  <bean name="b5">
    <factory>
      <call method="singletonList">
        <target>
          <class class="java.util.Collections"/>
        </target>
        <args>
          <const type="LONG" value="12"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b6">
    <factory>
      <call method="singletonList">
        <target>
          <class class="java.util.Collections"/>
        </target>
        <args>
          <ref ref="b5"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b7">
    <factory>
      <call method="singletonList">
        <target>
          <class class="java.util.Collections"/>
        </target>
        <args>
          <ref ref="b4"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b8">
    <factory>
      <call method="new">
        <target>
          <class class="java.util.ArrayList"/>
        </target>
        <initializers>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <const type="INT" value="2"/>
            </args>
          </call>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <const type="LONG" value="2"/>
            </args>
          </call>
        </initializers>
      </call>
    </factory>
  </bean>
</bean>
