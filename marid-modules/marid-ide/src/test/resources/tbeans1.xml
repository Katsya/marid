<bean name="beans">
  <factory>
    <null/>
  </factory>

  <bean name="b01">
    <factory>
      <string>str</string>
    </factory>
  </bean>

  <bean name="b02">
    <factory>
      <call method="valueOf">
        <target>
          <class class="java.math.BigInteger"/>
        </target>
        <args>
          <const type="LONG" value="1"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b03">
    <factory>
      <call method="new">
        <target>
          <class class="java.util.ArrayList"/>
        </target>
        <args>
          <const type="INT" value="10"/>
        </args>
        <initializers>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <const type="INT" value="2"/>
            </args>
          </call>
        </initializers>
      </call>
    </factory>
  </bean>

  <bean name="b04">
    <factory>
      <call method="size">
        <target>
          <call method="singletonList">
            <target>
              <class class="java.util.Collections"/>
            </target>
            <args>
              <const type="LONG" value="12"/>
            </args>
          </call>
        </target>
      </call>
    </factory>
  </bean>

  <bean name="b05">
    <factory>
      <call method="singletonList">
        <target>
          <class class="java.util.Collections"/>
        </target>
        <args>
          <const type="LONG" value="12"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b06">
    <factory>
      <call method="singletonList">
        <target>
          <class class="java.util.Collections"/>
        </target>
        <args>
          <ref ref="b05"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b07">
    <factory>
      <call method="singletonList">
        <target>
          <class class="java.util.Collections"/>
        </target>
        <args>
          <ref ref="b04"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b08">
    <factory>
      <call method="new">
        <target>
          <class class="java.util.ArrayList"/>
        </target>
        <initializers>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <const type="INT" value="2"/>
            </args>
          </call>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <const type="LONG" value="2"/>
            </args>
          </call>
        </initializers>
      </call>
    </factory>
  </bean>

  <bean name="b09">
    <factory>
      <call method="asList">
        <target>
          <class class="java.util.Arrays"/>
        </target>
        <args>
          <array>
            <elements>
              <call method="singletonList">
                <target>
                  <class class="java.util.Collections"/>
                </target>
                <args>
                  <const type="INT" value="1"/>
                </args>
              </call>
            </elements>
          </array>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b10">
    <factory>
      <array>
        <elements>
          <call method="singletonList">
            <target>
              <class class="java.util.Collections"/>
            </target>
            <args>
              <const type="LONG" value="1"/>
            </args>
          </call>
        </elements>
      </array>
    </factory>
  </bean>

  <bean name="b11">
    <factory>
      <array>
        <elements>
          <const type="I" value="1"/>
        </elements>
      </array>
    </factory>
  </bean>

  <bean name="b12">
    <factory>
      <call method="new">
        <target>
          <class class="java.util.ArrayList"/>
        </target>
        <initializers>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <array>
                <elements>
                  <const type="INT" value="1"/>
                </elements>
              </array>
            </args>
          </call>
          <call method="add">
            <target>
              <this/>
            </target>
            <args>
              <array>
                <elements>
                  <const type="LONG" value="1"/>
                </elements>
              </array>
            </args>
          </call>
        </initializers>
      </call>
    </factory>
  </bean>

  <bean name="b13">
    <factory>
      <call method="new">
        <target>
          <class class="org.marid.expression.mutable.testclasses.MyList"/>
        </target>
      </call>
    </factory>
  </bean>

  <bean name="b14">
    <factory>
      <call method="new">
        <target>
          <class class="org.marid.expression.mutable.testclasses.MyComplexBean"/>
        </target>
        <args>
          <const type="INT" value="1"/>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b15">
    <factory>
      <call method="asList">
        <target>
          <class class="java.util.Arrays"/>
        </target>
        <args>
          <array>
            <elements>
              <const type="I" value="1"/>
              <const type="J" value="2"/>
              <const type="D" value="2.0"/>
            </elements>
          </array>
        </args>
      </call>
    </factory>
  </bean>

  <bean name="b16">
    <factory>
      <call method="parseInt">
        <target>
          <class class="java.lang.Integer"/>
        </target>
        <args>
          <string>1</string>
        </args>
      </call>
    </factory>
  </bean>
</bean>
